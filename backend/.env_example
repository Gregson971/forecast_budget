# Configuration de la base de données
DATABASE_URL=postgresql://username:password@localhost:5432/forecast_budget

# Clé secrète pour JWT (générez une clé sécurisée)
SECRET_KEY=votre_clé_secrète_très_longue_et_complexe_ici

# Origines autorisées pour CORS (format JSON)
ORIGINS_ALLOWED=["http://localhost:3000"]

# Configuration PostgreSQL (pour Docker)
POSTGRES_DB=forecast_budget
POSTGRES_USER=postgres
POSTGRES_PASSWORD=password

# Configuration pour le développement
DEBUG=true
ENVIRONMENT=development 